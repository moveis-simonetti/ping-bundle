{
  "name": "simonetti/ping-bundle",
  "type": "symfony-bundle",
  "description": "Ping Bundle",
  "keywords": ["ping", "bundle"],
  "homepage": "https://github.com/moveis-simonetti/ping-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Project Contributors",
      "homepage": "https://github.com/moveis-simonetti/ping-bundle/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.0.0|^8",
    "symfony/config": "^3|^4",
    "symfony/console": "^3|^4",
    "symfony/framework-bundle": "^3|^4",
    "symfony/http-foundation": "^3|^4",
    "symfony/http-kernel": "^3|^4",
    "symfony/lock": "^3|^4"
  },
  "require-dev": {
    "psr/log": "~1.0",
    "symfony/debug": "*",
    "symfony/phpunit-bridge": "*",
    "symfony/var-dumper": "*",
    "symfony/yaml": "*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Simonetti\\PingBundle\\": "./"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Simonetti\\PingBundle\\Tests\\": "Tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
